all:  monkey-lua.so
include ../Make.common

CC	= @echo "  CC   $(_PATH)/$@"; $CC
CC_QUIET= @echo -n; $CC
CFLAGS	= $CFLAGS -I/usr/include/lua5.2
LDFLAGS = $LDFLAGS -llua5.2
DEFS    = $DEFS
OBJECTS = mk_lua.o mk_lua_env.o mk_lua_config.o mk_lua_request.o mk_lua_util.o mk_lua_cookie.o

-include $(OBJECTS:.o=.d)

monkey-lua.so: $(OBJECTS)
	$(CC) $(CFLAGS) $(DEFS) -shared -o $@ $^ -lc $(LDFLAGS) 
